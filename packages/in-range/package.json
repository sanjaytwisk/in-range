    "babel-loader": "8.1.0",
    "jest": "25.2.6",
    "parcel": "1.12.4",
    "postcss-cli": "7.1.0",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.2.1",
    "postcss-preset-env": "6.7.0",
    "ts-jest": "25.3.0",
    "typescript": "3.8.3"
  }
}
